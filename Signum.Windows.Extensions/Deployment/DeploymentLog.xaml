<UserControl x:Class="Signum.Windows.Deployment.DeploymentLog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:d="clr-namespace:Signum.Entities.Deployment;assembly=Signum.Entities.Extensions"
    m:Common.TypeContext="d:DeploymentLogDN"
    MinWidth="300">
    <StackPanel>
        <m:ValueLine m:Common.Route="CreationDate" />
        <m:ValueLine m:Common.Route="Version" />
        <m:ValueLine m:Common.Route="Description" />
        <m:ValueLine m:Common.Route="MachineName" />
        <m:ValueLine m:Common.Route="DatabaseName" />
        <m:ValueLine m:Common.Route="DataSourceName" />
    </StackPanel>
</UserControl>
