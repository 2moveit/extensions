<UserControl x:Class="Signum.Windows.Extensions.SMS.SMSMessage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:d="clr-namespace:Signum.Entities.SMS;assembly=Signum.Entities.Extensions"
    m:Common.TypeContext="d:SMSMessageDN"
    MinWidth="300">
    <StackPanel>
        <m:EntityLine m:Common.Route="Template" />
        <m:ValueLine m:Common.Route="Message" />
        <m:ValueLine m:Common.Route="From" />
        <m:ValueLine m:Common.Route="SendDate" />
        <m:ValueLine m:Common.Route="State" />
        <m:ValueLine m:Common.Route="DestinationNumber" />
        <m:ValueLine m:Common.Route="MessageID" />
        <m:EntityCombo m:Common.Route="SendPackage" />
        <m:EntityCombo m:Common.Route="UpdatePackage" />
    </StackPanel>
</UserControl>
