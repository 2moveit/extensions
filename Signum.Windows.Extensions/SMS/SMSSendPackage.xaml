<UserControl x:Class="Signum.Windows.SMS.SMSSendPackage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:d="clr-namespace:Signum.Entities.SMS;assembly=Signum.Entities.Extensions"
    m:Common.TypeContext="d:SMSSendPackageDN"
    MinWidth="300">
    <StackPanel>
        <m:ValueLine m:Common.Route="Name" />
        <m:ValueLine m:Common.Route="NumLines" />
        <m:ValueLine m:Common.Route="NumErrors" />
        <m:SearchControl QueryName="{x:Type d:SMSMessageDN}" FilterColumn="Entity.SendPackage"/>
    </StackPanel>
</UserControl>
