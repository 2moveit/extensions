<UserControl x:Class="Signum.Windows.Processes.ProcessExecution"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:d="clr-namespace:Signum.Entities.Processes;assembly=Signum.Entities.Extensions"
    m:Common.TypeContext="d:ProcessExecutionDN"
    MinWidth="300">
    <StackPanel m:Common.IsReadOnly="True">
        <m:ValueLine m:Common.Route="State" />
        <m:EntityCombo m:Common.Route="Process" />
        <m:EntityCombo m:Common.Route="ProcessData" />
        <m:ValueLine m:Common.Route="CreationDate" />
        <m:ValueLine m:Common.Route="PlannedDate" m:Common.CollapseIfNull="True" />
        <m:ValueLine m:Common.Route="CancelationDate" m:Common.CollapseIfNull="True" />
        <m:ValueLine m:Common.Route="QueuedDate" m:Common.CollapseIfNull="True" />
        <m:ValueLine m:Common.Route="ExecutionStart" m:Common.CollapseIfNull="True" />
        <m:ValueLine m:Common.Route="ExecutionEnd" m:Common.CollapseIfNull="True" />
        <m:ValueLine m:Common.Route="SuspendDate" m:Common.CollapseIfNull="True" />
        <m:ValueLine m:Common.Route="ErrorDate" m:Common.CollapseIfNull="True" />
        <m:ValueLine m:Common.Route="Exception" m:Common.CollapseIfNull="True" />
        <m:ValueLine m:Common.Route="Progress" m:Common.CollapseIfNull="True" />
    </StackPanel>
</UserControl>
