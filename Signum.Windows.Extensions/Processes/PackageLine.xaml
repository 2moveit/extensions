<UserControl x:Class="Signum.Windows.Processes.PackageLine"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:d="clr-namespace:Signum.Entities.Processes;assembly=Signum.Entities.Extensions"
    m:Common.TypeContext="d:PackageLineDN"
    MinWidth="300">
    <StackPanel>
        <m:EntityCombo m:Common.Route="Package" />
        <m:EntityCombo m:Common.Route="Entity" />
        <m:ValueLine m:Common.Route="FinishTime" />
        <m:ValueLine m:Common.Route="Exception" />
    </StackPanel>
</UserControl>
