<UserControl x:Class="Signum.Windows.ControlPanels.PanelPartEdit"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:cp="clr-namespace:Signum.Entities.ControlPanel;assembly=Signum.Entities.Extensions"
    m:Common.TypeContext="cp:PanelPartEdit"
    MinWidth="600">
    <StackPanel>
        <m:ValueLine m:Common.Route="Title"/>
        <m:ValueLine m:Common.Route="Row"/>
        <m:ValueLine m:Common.Route="Column"/>
        <m:EntityDetail m:Common.Route="Content"/>
    </StackPanel>
</UserControl>
