<UserControl x:Class="Signum.Windows.UserQueries.QueryTokenDNBuilder"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows">
    <StackPanel>
        <m:QueryTokenBuilder Token="{Binding TryToken}" SubTokensEvent="QueryTokenBuilder_SubTokensEvent" x:Name="tokenBuilder"/>
        <StackPanel Visibility="{Binding ParseException, Converter={x:Static m:Converters.NullToVisibility}}">
            <TextBlock Foreground="Red" FontWeight="Black"  Text="{Binding TokenString}"/>
        </StackPanel>
    </StackPanel>
</UserControl>
