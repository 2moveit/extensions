<UserControl x:Class="Signum.Windows.Excel.ExcelReport"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:dq="clr-namespace:Signum.Entities.Reports;assembly=Signum.Entities.Extensions"
    m:Common.TypeContext="dq:ExcelReportDN" MinWidth="300">
    <StackPanel>
        <m:ValueLine LabelText="{m:Loc QueryName}" m:Common.Route="QueryName"  m:Common.IsReadOnly="true" />
        <m:ValueLine LabelText="{m:Loc Name}" m:Common.Route="DisplayName" />
		<m:FileLine LabelText="{m:Loc File}" m:Common.Route="File" />
        <m:ValueLine LabelText="{m:Loc Removed}" m:Common.Route="Deleted" />
	</StackPanel>
</UserControl>
