<UserControl x:Class="Signum.Windows.Scheduler.CustomTaskExecution"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:Signum.Windows;assembly=Signum.Windows"
    xmlns:d="clr-namespace:Signum.Entities.Scheduler;assembly=Signum.Entities.Extensions"
    m:Common.TypeContext="d:CustomTaskExecutionDN"
    MinWidth="300">
    <StackPanel m:Common.IsReadOnly="True">
        <m:EntityCombo m:Common.Route="CustomTask" />
        <m:ValueLine m:Common.Route="StartTime" />
        <m:ValueLine m:Common.Route="EndTime" />
        <m:ValueLine m:Common.Route="Exception" />
    </StackPanel>
</UserControl>
