@using Signum.Entities.Deployment

@using (var e = Html.TypeContext<DeploymentLogDN>())
{   
    @Html.ValueLine(e, f => f.CreationDate)
    @Html.ValueLine(e, f => f.DatabaseName)
    @Html.ValueLine(e, f => f.MachineName)
    @Html.ValueLine(e, f => f.Version)
    @Html.ValueLine(e, f => f.Description)
}