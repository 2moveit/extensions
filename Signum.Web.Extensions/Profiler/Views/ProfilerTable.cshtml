@using Signum.Utilities.ExpressionTrees
@using Signum.Web.Profiler
@model List<HeavyProfilerEntry>

<table class="sf-search-results sf-profiler-table">
    <thead>
        <tr>
            <th>
                Entry
            </th>
            <th>
                Role
            </th>
            <th>
                Time
            </th>
             <th>
                Method
            </th>
            <th>
                Aditional Data
            </th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            var entry = Model[i];
            <tr>
                <td>
                    @Html.ProfilerEntry("View", entry.FullIndex())
                </td>
                <td>
                    @entry.Role
                </td>
                <td align="right">
                    @entry.Elapsed.NiceToString()
                </td>
                <td>
                    @(entry.Type.TypeName()).@entry.Method.Name
                </td>
                <td>
                    @entry.AditionalData.TryCC(o => o.ToString().TryLeft(50))
                </td>
            </tr>
        }
    </tbody>
</table>
<br />
