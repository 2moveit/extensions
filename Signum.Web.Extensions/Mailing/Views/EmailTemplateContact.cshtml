@using Signum.Entities.Mailing
@using Signum.Entities.DynamicQuery
@using Signum.Web.UserQueries
@Html.ScriptCss("~/Mailing/Content/SF_Mailing.css")
@using (var tc = Html.TypeContext<EmailTemplateContactDN>())
{
    using (var qtTc = tc.SubContext(etTc => etTc.Token))
    {
        @Html.Field("Email Owner", Html.QueryTokenDNBuilder(qtTc, (QueryDescription)ViewData[ViewDataKeys.QueryDescription]))
    }
    @Html.ValueLine(tc, c => c.EmailAddress)
    @Html.ValueLine(tc, c => c.DisplayName)
}
@Html.ScriptsJs("~/Mailing/Scripts/SF_Mailing.js")


