@using Signum.Entities.Mailing
@using Signum.Entities.Processes

@using (var nc = Html.TypeContext<NewsletterDeliveryDN>()) 
{
	@Html.ValueLine(nc, n => n.Sent)
	@Html.ValueLine(nc, n => n.SendDate)
	@Html.EntityLine(nc, n => n.Recipient)
	@Html.EntityLine(nc, n => n.Newsletter)

    @Html.SearchControl(new FindOptions(typeof(ProcessExceptionLineDN), "Line", nc.Value), new Context(nc, "Exceptions"))
}
