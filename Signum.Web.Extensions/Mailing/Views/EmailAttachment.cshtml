@using Signum.Entities.Mailing
@using Signum.Web.Files

@using (var sc = Html.TypeContext<EmailAttachmentDN>())
{
    @Html.FileLine(sc, ea => ea.File, fl => { fl.FileType = EmailFileType.Attachment; fl.LabelVisible = false; })

    @Html.ValueLine(sc, c => c.Type, vl => { vl.ReadOnly = true; vl.WriteHiddenOnReadonly = true; })
}
